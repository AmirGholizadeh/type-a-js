{
    "parser":"@typescript-eslint/parser",
    "plugins":[
        "@typescript-eslint",
        "prettier"
    ],
    "env": {"browser":true},
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "prettier/prettier":"error"
    },
    "parserOptions": {
        "project":"./tsconfig.json",
        "ecmaFeatures": {
            "impliedStrict": true
        },
        "ecmaVersion": 2020,
        "sourceType": "module"  
      
    }
}